<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="512.0" prefWidth="479.0" stylesheets="@../ressources/css/login.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="allforkids.controllers.ForgetPasswordController">
   <children>
      <Label fx:id="numeroLabel" layoutX="8.0" layoutY="119.0" prefHeight="33.0" prefWidth="343.0" text="Label" />
      <JFXButton fx:id="verifierEmailButton" layoutX="320.0" layoutY="26.0" onAction="#verifierEmailAction" styleClass="login-button" text="Verifier email" />
      <JFXTextField fx:id="codeTextField" layoutX="138.0" layoutY="205.0" onKeyReleased="#showPassFields" promptText="Veillez Saisir le code ici" />
      <JFXPasswordField fx:id="password" focusColor="#de7f21" layoutX="140.0" layoutY="282.0" promptText="New Password" styleClass="text-field" unFocusColor="#757272" />
      <JFXPasswordField fx:id="rpassword" focusColor="#de7f21" layoutX="140.0" layoutY="368.0" onKeyReleased="#checkPass" promptText="Repeat New Password" styleClass="text-field" unFocusColor="#757272" />
      <JFXButton fx:id="saveButton" layoutX="187.0" layoutY="446.0" onAction="#setNewPasswordAction" styleClass="login-button" text="Sauvegarder" />
      <JFXTextField fx:id="emailTextField" layoutX="57.0" layoutY="23.0" prefHeight="39.0" prefWidth="238.0" promptText="Entrez votre adresse mail" />
      <JFXButton fx:id="sendCodeButton" layoutX="355.0" layoutY="119.0" onAction="#envoyerCodeAction" styleClass="login-button" text="Envoyer Code" />
   </children>
</AnchorPane>
